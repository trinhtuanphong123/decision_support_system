[run]
source = .
omit = 
    */test/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    */htmlcov/*
    */.pytest_cache/*
branch = True

[report]
precision = 2
show_missing = True
skip_covered = False
skip_empty = True

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    pass

[html]
directory = htmlcov

[xml]
output = coverage.xml